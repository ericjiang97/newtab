{
    "manifest_version": 2,
    "name": "New Tab",
    "version": "1.0",
    "description": "New Tab",

    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://cdnjs.cloudflare.com; object-src 'self'",
    "permissions": [
        "activeTab",
        "history",
        "https://ajax.googleapis.com/",
        "*://*.bom.gov.au/*",
        "geolocation"
    ]
}